  "dependencies": {
    "@slack/interactive-messages": "^1.4.1",
    "@slack/rtm-api": "^5.0.3",
    "@types/cheerio": "^0.22.24",
    "@types/morgan": "^1.9.2",
    "@types/node-fetch": "^2.5.4",
    "@types/puppeteer": "^2.0.0",
    "aws-sdk": "^2.603.0",
    "cheerio": "^1.0.0-rc.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
